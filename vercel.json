{
    "version": 2,
    "builds": [
        {
            "src": "api/webhook.py",
            "use": "@vercel/python"
        }
    ],
    "routes": [
        {
            "src": "/webhook",
            "dest": "api/webhook.py"
        }
    ],
    "functions": {
        "main.py": {
            "memory": 1024,
            "maxDuration": 10
        }
    }
} 